<h1>Listing topics</h1>

<table>
  <thead>
    <tr>
      <th>Priorityconditionid</th>
      <th>Topictitle</th>
      <th>Intervention</th>
      <th>Phase</th>
      <th>Phasedetails</th>
      <th>Comparators</th>
      <th>Healthimpact</th>
      <th>Patientpopulation</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @topics.each do |topic| %>
      <tr>
        <td><%= topic.priorityConditionID %></td>
        <td><%= topic.topicTitle %></td>
        <td><%= topic.intervention %></td>
        <td><%= topic.phase %></td>
        <td><%= topic.phaseDetails %></td>
        <td><%= topic.comparators %></td>
        <td><%= topic.healthImpact %></td>
        <td><%= topic.patientPopulation %></td>
        <td><%= topic.status %></td>
        <td><%= link_to 'Show', topic %></td>
        <td><%= link_to 'Edit', edit_topic_path(topic) %></td>
        <td><%= link_to 'Destroy', topic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Topic', new_topic_path %>
